{
  "author": "Selleo Team",
  "name": "stylelint-config-selleo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "exports": {
    "./bem": "./stylelint-config-selleo/bem/.stylelintrc.json",
    "./core": "./stylelint-config-selleo/core/.stylelintrc.json",
    "./sass": "./stylelint-config-selleo/sass/.stylelintrc.json"
  },
  "keywords": [
    "stylelint-config",
    "stylelint",
    "selleo"
  ],
  "scripts": {
    "lint-all": "stylelint **/*.{scss,css} --fix",
    "fix-current-changes": "git diff origin/master --name-only  --diff-filter=AM -- '*.scss' '*.css' | tail | xargs -r stylelint --fix --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Selleo/stylelint-config-selleo.git"
  },
